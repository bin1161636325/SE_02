}
.over_zi{
  position: absolute;
  width: 400rpx;
  height: 200rpx;
  z-index: 3;
  margin-top:200rpx;
  -webkit-transform: rotate(355deg);
          transform: rotate(355deg);
  margin-left: 200rpx;
}
.king{
  position: absolute;
  margin-top:310rpx;
  -webkit-transform: rotate(342deg);
          transform: rotate(342deg);
  margin-left: 60rpx;
  width: 140rpx;
